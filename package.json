  "scripts": {
    "start": "electron .",
    "start-bootstrap": "electron . --theme=bootstrap",
    "start-dark": "electron . --theme=dark",
    "start-light": "electron . --theme=light",
    "gulp": "gulp",
    "app": "gulp app && cd app && electron .",
    "app-pack": "gulp app && cd app && electron-packager . --overwrite --platform=win32 --arch=all --icon=build/icon.ico --prune=true --out=dist --app-copyright=\"Copyright Â© 2017-2019 Jan K. Pluta\"--win32metadata.CompanyName=JKP --win32metadata.FileDescription=\"Eksperymentalna strona startowa\" --win32metadata.ProductName=\"Jan K. Pluta\"",
    "app-setup": "electron-installer-windows --src app/dist/jkpluta-electron-win32-x64/ --dest dist --icon build/icon.ico",
    "www": "gulp www && cd www && http-server -a localhost -p 60080 -o -c-1",
    "cordova": "gulp cordova",
    "iis": "gulp iis",
    "nginx": "sudo gulp nginx"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
